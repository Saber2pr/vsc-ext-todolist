{
  "manifest_version": 2,
  "name": "TodolistTreeView",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    }
  },
  "permissions": [
    "webRequest",
    "storage",
    "webRequestBlocking",
    "browsingData",
    "<all_urls>"
  ],
  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  }
}
